repeat wait() until game:IsLoaded();local sWF=true;spawn(function()while wait(3)and sWF do pcall(function()local c=game:GetService("GuiService"):GetErrorCode().Value if c==1 or(c>=Enum.ConnectionError.DisconnectErrors.Value and c<Enum.ConnectionError.PlacelaunchOtherError.Value)then sWF=false end end)pcall(function()if sWF then local p=game.Players.LocalPlayer local ok,sv=pcall(function()return workspace:GetServerTimeNow()end) local ts=(ok and type(sv)=="number")and sv or os.time() writefile(p.Name.."_checkyum.json",string.format('{"playerName":"%s","time":"%s"}',p.Name,os.date("!%Y-%m-%dT%H:%M:%SZ",ts)))end end)end end)
