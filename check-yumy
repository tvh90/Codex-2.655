repeat wait() until game:IsLoaded() and game:GetService("Players").LocalPlayer;local sWF=true;spawn(function()while wait(3)and sWF do pcall(function()local c=game:GetService("GuiService"):GetErrorCode().Value if c==1 or c==769 or(c>=Enum.ConnectionError.DisconnectErrors.Value and c<Enum.ConnectionError.PlacelaunchOtherError.Value)then sWF=false end end);if sWF then pcall(function()writefile(game:GetService("Players").LocalPlayer.Name.."_checkyum.json",string.format('{"playerName":"%s","time":"%s"}',game:GetService("Players").LocalPlayer.Name,os.date("!%Y-%m-%dT%H:%M:%SZ",workspace:GetServerTimeNow())))end)end end end)
